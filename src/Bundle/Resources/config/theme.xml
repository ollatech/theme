<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
	<services>
		<service id="Olla\Theme\View" class="Olla\Theme\View">
			<argument type="service" id="twig" />
			<argument type="service" id="Olla\Theme\Design" />
			<argument type="service" id="olla.metadata" />
			<tag name="olla.format" alias="html" />
		</service>
		<service id="Olla\Theme\Design" class="Olla\Theme\Design">
			<argument type="service" id="service_container" />
			<argument type="service" id="twig" />
			<argument type="service" id="olla.finder" />
		</service>
	</services>
</container>